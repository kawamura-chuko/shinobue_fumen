    <%= link_to root_path, class: 'navbar-brand' do %>
      <%= image_tag 'logo.png' %>
    <% end %>
    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
      <%= form_with(url: new_sheet_path, method: :get) do |form| %>
        <li class="nav-item">
          <%= form.submit t('defaults.make_music'), class: 'btn btn-primary btn-lg' %>
        </li>
        <li class="nav-item">
          <% level = @sheet ? @sheet.level : Sheet::DEFAULT_LEVEL %>
          <%= form.select :level, options_for_select(Sheet.types_of_selectable_levels, selected: level), {}, { class: 'form-control' } %>
        </li>
      <% end %>
    </ul>